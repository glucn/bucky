{
  "name": "bucky",
  "productName": "Bucky",
  "version": "1.0.0",
  "description": "Personal bookkeeping software",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "dev:webpack": "webpack serve --config webpack.renderer.config.js",
    "dev:electron": "cross-env NODE_ENV=development electron-forge start",
    "dev": "concurrently -k \"npm run dev:webpack\" \"wait-on tcp:3001 && npm run dev:electron\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "tsc && electron-forge make",
    "watch": "tsc -w",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsc && node prisma/seed.js"
  },
  "keywords": [
    "bookkeeping",
    "finance",
    "accounting"
  ],
  "author": {
    "name": "Gary Lu",
    "email": "gary.yue.lu@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.9",
    "@babel/preset-typescript": "^7.23.9",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-fuses": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@electron/fuses": "^1.8.0",
    "@prisma/client": "^6.9.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20.11.19",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "^28.2.3",
    "electron-is-dev": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "postcss": "^8.5.5",
    "postcss-loader": "^8.1.0",
    "prisma": "^6.9.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "express": "^5.1.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "sqlite3": "^5.1.7"
  }
}
